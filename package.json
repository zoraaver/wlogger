{
  "name": "wlogger",
  "version": "1.0.0",
  "description": "log and track workouts",
  "main": "dist/server.js",
  "scripts": {
    "test": "jest --maxWorkers 1",
    "start": "npx tsc && node dist/server.js",
    "dev": "tsc -w & nodemon"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zoraaver/wlogger.git"
  },
  "author": "zoraaver",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/zoraaver/wlogger/issues"
  },
  "nodemonConfig": {
    "delay": "2000",
    "restartable": "rs",
    "ext": "js",
    "watch": [
      "dist"
    ],
    "ignore": [
      "src",
      ".git",
      "node_modules"
    ],
    "exec": "node dist/server.js"
  },
  "homepage": "https://github.com/zoraaver/wlogger#readme",
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/cors": "^2.8.10",
    "@types/express": "^4.17.11",
    "@types/jest": "^26.0.22",
    "@types/jsonwebtoken": "^8.5.1",
    "@types/mongodb": "^3.6.10",
    "@types/mongoose": "^5.10.4",
    "@types/morgan": "^1.9.2",
    "@types/node": "^14.14.36",
    "@types/supertest": "^2.0.10",
    "@types/validator": "^13.1.3",
    "jest": "^26.6.3",
    "morgan": "^1.10.0",
    "nodemon": "^2.0.7",
    "supertest": "^6.1.3",
    "ts-jest": "^26.5.4",
    "typescript": "^4.2.3"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "google-auth-library": "^7.0.3",
    "helmet": "^4.4.1",
    "jsonwebtoken": "^8.5.1",
    "mongodb": "^3.6.5",
    "mongoose": "^5.12.2",
    "validator": "^13.5.2"
  }
}
